complete <- function(directory, id = 1:332) {
#creates a list of files
files_list <- list.files(directory, full.names=TRUE)
complete_cases <- data.frame();
for(index in id){
data <- read.csv(files_list[index], comment.char = "")
# complete.cases(data.frame) is used to create logical vector
# where obs/row is good
c <- complete.cases(data);
complete_cases <- rbind(complete_cases, c(index, nrow(data[c,])));
}
names(complete_cases) <- c("id", "nobs");
complete_cases;
}
