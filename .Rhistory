ids <- complete_cases$id[nobs>threshold]
files_list <- list.files(directory, pattern="*.csv", full.names=TRUE)
dat <- data.frame ()
corr_vector <-data.frame()
for (i in ids){
dat <- (read.csv(files_list[i], header=TRUE))
corr_vector<-cor(dat$sulfate, dat$nitratre)
}
corr_vector
}
